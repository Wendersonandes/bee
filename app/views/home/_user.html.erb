  <%users.in_groups_of(3, false).each do |group| %>
  <div class="w3-row-padding">
    <% group.each do |item| %>
    <div class="w3-third w3-container w3-margin-bottom">
      <%= link_to (image_tag item.avatar.url(:medium), class:"w3-hover-opacity", style:'width:100%'), profile_path(item.user_name) %>
      <div class="w3-container w3-white">
        <br>
        <p><b><%= item.completo %></b></p>
      </div>
    </div>
    <% end %>
  </div>
  <% end %>