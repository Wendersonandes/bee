   <% printers.each do |printer| %>
   <div id='print_<%= printer.id %>'>
    <a href=<%= edit_printer_path(printer)%> class="media list-group-item" id="bleee">
    <span class="pull-left">
    <%= image_tag printer.user.avatar.url(:medium), class: "media-object img-circle img-notification"%>
    </span>
    <span class="media-body" style="">
        <div>
        <h4 style="font-weight:bold;position: relative;font-size: 15px;">
        <%= printer.user.name %>
            <i class="fa fa-star" aria-hidden="true"></i>
            <i class="fa fa-star" aria-hidden="true"></i>
            <i class="fa fa-star" aria-hidden="true"></i>
            <i class="fa fa-star" aria-hidden="true"></i>
            <i class="fa fa-star" aria-hidden="true"></i>
        
        </h4>
        </div>
        <p> Marca: <%= printer.marca %> </p>
        <p >Modelo: <%= printer.modelo %></p>
        <p>Material: <% printer.materials.each do |material|%>
        	<%= material.name %>/
        <% end %>
        </p>
        <p>Entrega: 3 Dias</p>
        
        <!-- <%= render printer.materials %>
        <small class="text-muted"><%= time_ago_in_words printer.created_at%></small>

         -->

    </span>
</a>
    <span>
    	<%= link_to printer_path(printer), method: :delete, data: { confirm: "Are you sure?" }, remote: true do %>
  			<span class="glyphicon glyphicon-remove delete-comment"></span>
		<% end %>
    </span>
        <%= render 'profiles/active_or', printer: printer %>



    </div>
<% end %>


<style type="text/css">
    
    #bleee{

       
        border-radius: 10px;
    }
</style>